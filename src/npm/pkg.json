{
  "name": "@lonu/stc",
  "version": "2.14.3",
  "description": "A tool for converting OpenApi/Swagger/Apifox into code.",
  "type": "module",
  "module": "esm/mod.js",
  "types": "esm/mod.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/long-woo/stc.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "api",
    "doc",
    "code-generation",
    "apifox",
    "stc",
    "postman",
    "apipost"
  ],
  "author": "lonu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/long-woo/stc/issues"
  },
  "homepage": "https://github.com/long-woo/stc#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./esm/mod.d.ts",
        "default": "./esm/mod.js"
      },
      "require": {
        "types": "./esm/mod.d.ts",
        "default": "./esm/mod.js"
      }
    },
    "./plugin/definition": {
      "import": "./esm/src/plugins/definition.js",
      "types": "./esm/src/plugins/definition.d.ts"
    },
    "./plugin/action": {
      "import": "./esm/src/plugins/action.js",
      "types": "./esm/src/plugins/action.d.ts"
    },
    "./plugin/types": {
      "types": "./esm/src/plugins/typeDeclaration.d.ts"
    },
    "./plugin/common": {
      "import": "./esm/src/plugins/common.js",
      "types": "./esm/src/plugins/common.d.ts"
    }
  }
}
