{
  "name": "@loongwoo/stc",
  "version": "1.4.0",
  "description": "OpenAPI(Swagger)、Apifox 文档转换为接口文件",
  "type": "module",
  "module": "esm/mod.js",
  "types": "esm/mod.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/long-woo/stc.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "api",
    "doc",
    "code-generation",
    "apifox",
    "stc"
  ],
  "author": "loong.woo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/long-woo/stc/issues"
  },
  "homepage": "https://github.com/long-woo/stc#readme",
  "files": [
    "esm",
    "script"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./esm/mod.d.ts",
        "default": "./esm/mod.js"
      },
      "require": {
        "types": "./esm/mod.d.ts",
        "default": "./esm/mod.js"
      }
    }
  }
}
